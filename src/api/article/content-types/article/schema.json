{
  "kind": "collectionType",
  "collectionName": "articles",
  "info": {
    "singularName": "article",
    "pluralName": "articles",
    "displayName": "文章",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "content": {
      "type": "text"
    },
    "imgs": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "introduction": {
      "type": "string"
    },
    "publisher": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "articles"
    },
    "comment_cs": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::comment-c.comment-c",
      "mappedBy": "article"
    },
    "likes": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::like.like",
      "mappedBy": "article"
    },
    "classification": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::classification.classification",
      "inversedBy": "article"
    },
    "labels": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::label.label",
      "inversedBy": "articles"
    },
    "aboutDynamic": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::trend.trend",
      "mappedBy": "aboutArticle"
    },
    "cover": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    }
  }
